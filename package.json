{
  "name": "sigmacode2",
  "version": "1.0.0",
  "description": "SIGMACODE AI â€“ Security-First AI Platform. Enterprise AI automation with built-in firewall protection, SOC 2 compliance, and comprehensive security features.",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit -p tsconfig.typecheck.json",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "drizzle:generate": "bash -lc 'set -a; source .env; set +a; drizzle-kit generate'",
    "drizzle:migrate": "bash -lc 'set -a; source .env; set +a; drizzle-kit migrate'",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --run",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:jest:ci": "jest --ci --coverage --watchAll=false",
    "e2e": "playwright test -c e2e/playwright.config.ts",
    "e2e:ui": "playwright test -c e2e/playwright.config.ts --ui",
    "e2e:report": "playwright show-report",
    "e2e:install": "playwright install",
    "rt:gen": "tsx scripts/redteam/generator.ts --in scripts/redteam/prompts/jailbreak.txt --out .rt_jailbreak.jsonl --mutations 2",
    "rt:run": "tsx scripts/redteam/harness.ts --prompts .rt_jailbreak.jsonl --endpoint http://localhost:3000/api/agents/test-agent/invoke --mode shadow --concurrency 3 --out .rt_results.jsonl",
    "rt:score": "tsx scripts/redteam/score.ts --in .rt_results.jsonl --out .rt_scores.csv",
    "rt:smoke": "pnpm rt:gen && pnpm rt:run && pnpm rt:score",
    "prepare": "husky",
    "format": "prettier --cache --write .",
    "lint-staged": "lint-staged"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "postgresql",
    "drizzle-orm"
  ],
